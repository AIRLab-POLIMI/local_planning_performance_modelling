<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <arg name="localization_params_file"/>
    <arg name="output" default="screen"/>

    <param name="/use_sim_time" value="true"/>

    <node
        name="amcl"
        pkg="amcl"
        type="amcl"
        respawn="false"
        required="true"
        output="$(arg output)">
        <rosparam file="$(arg localization_params_file)"/>
    </node>

</launch>
